<?xml version="1.0" ?>
<DescriptionSetTemplate  xmlns="http://dublincore.org/xml/dc-dsp/2008/03/31">

  <DescriptionTemplate ID="SSEInitiative" minOccur="1" maxOccur="infinity" standalone="yes">
    <ResourceClass>http://purl.org/essglobal/vocab/v1.0/SSEInitiative</ResourceClass>
      
      <StatementTemplate ID="Name"  minOccurs="1" maxOccurs="1" type="literal">
	<Property>http://purl.org/goodrelations/v1#name</Property>
      </StatementTemplate>

      <StatementTemplate ID="Description"  minOccurs="0" maxOccurs="1" type="literal">
	<Property>http://www.purl.org/dc/terms/description</Property>
	<LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
      </StatementTemplate>

      <StatementTemplate ID="Objective"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/objective</Property>
	<LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
      </StatementTemplate>
	
      <StatementTemplate ID="Mission"  minOccurs="0" maxOccurs="1" type="literal">
	 <Property>http://purl.org/essglobal/vocab/v1.0/mission</Property>
	 <LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
      </StatementTemplate>
	
      <StatementTemplate ID="Type"  minOccurs="0" maxOccurs="infinity" type="nonliteral">
	  <Property>http://www.purl.org/dc/terms/type</Property>
	  <NonLiteralConstraint> 
	    <VocabularyEncodingSchemeURI>http://purl.org/essglobal/standard/themes/</VocabularyEncodingSchemeURI>
	  </NonLiteralConstraint>
      </StatementTemplate>
      
       <StatementTemplate ID="Economic-Sector"  minOccurs="0" maxOccurs="infinity" type="nonliteral">
	  <Property>http://www.purl.org/essglobal/vocab/v1.0/economicSector</Property>
	  <NonLiteralConstraint> 
	    <VocabularyEncodingSchemeURI>http://purl.org/essglobal/standard/activities/>VocabularyEncodingSchemeURI>
	  </NonLiteralConstraint>
      </StatementTemplate>
      
       <StatementTemplate ID="totalWomen"  minOccurs="0" maxOccurs="1" type="literal">
	 	 <Property>http://purl.org/essglobal/vocab/v1.0/totalWomen</Property>
      </StatementTemplate>
      
      <StatementTemplate ID="totalMen"  minOccurs="0" maxOccurs="1" type="literal">
	  		<Property>http://purl.org/essglobal/vocab/v1.0/TotalMen</Property>
      </StatementTemplate>
      
      <StatementTemplate ID="Total-of-Members"  minOccurs="0" maxOccurs="1" type="literal">
	  		<Property>http://purl.org/essglobal/vocab/v1.0/totalOfMembers</Property>
      </StatementTemplate>
      
       <StatementTemplate ID="URL"  minOccurs="0" maxOccurs="1" type="literal">
			<Property>http://purl.org/goodrelations/v1#homepage</Property>
      </StatementTemplate>
      
      <StatementTemplate ID="Legal-Form"  minOccurs="0" maxOccurs="1" type="nonliteral">
			 <Property>http://purl.org/essglobal/vocab/v1.0/legalForm</Property>
		    <NonLiteralConstraint> 
	    		<VocabularyEncodingSchemeURI>http://purl.org/essglobal/standard/legal-form/</VocabularyEncodingSchemeURI>
	  		 </NonLiteralConstraint>
      </StatementTemplate>
                
      <StatementTemplate ID="has-Product-or-Service" minOccurs="0" maxOccurs="infinity" type="nonliteral">
            <Property>http://purl.org/essglobal/vocab/v1.0/hasProductOrService</Property>
            <NonLiteralConstraint descriptionTemplateRef="Product-or-Service"/>
      </StatementTemplate>
      
      <StatementTemplate ID="Is-part-of-a-Network" minOccurs="0" maxOccurs="infinity" type="nonliteral">
            <Property>http://purl.org/dc/terms/isPartOf</Property>
            <NonLiteralConstraint descriptionTemplateRef="Network"/>
      </StatementTemplate>

       </DescriptionTemplate> 

<DescriptionTemplate ID="Network" minOccur="0" maxOccur="infinity" standalone="yes">
    <ResourceClass>http://purl.org/essglobal/vocab/v1.0/Network</ResourceClass>
    
	<StatementTemplate ID="Name"  minOccurs="1" maxOccurs="1" type="literal">
		<Property>http://purl.org/goodrelations/v1#name</Property>
   </StatementTemplate>
      
     <StatementTemplate ID="Description"  minOccurs="0" maxOccurs="1" type="literal">
	<Property>http://www.purl.org/dc/terms/description</Property>
		<LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
      </StatementTemplate>

      <StatementTemplate ID="Objective"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/objective</Property>
	  <LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
      </StatementTemplate>
	
      <StatementTemplate ID="Mission"  minOccurs="0" maxOccurs="1" type="literal">
	 <Property>http://purl.org/essglobal/vocab/v1.0/mission</Property>
	 <LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
      </StatementTemplate>
      
      <StatementTemplate ID="URL"  minOccurs="0" maxOccurs="1" type="literal">
			<Property>http://purl.org/goodrelations/v1#homepage</Property>
      </StatementTemplate>
      
       <StatementTemplate ID="Legal-Form"  minOccurs="0" maxOccurs="1" type="nonliteral">
			 <Property>http://purl.org/essglobal/vocab/v1.0/legalForm</Property>
		    <NonLiteralConstraint> 
	    		<VocabularyEncodingSchemeURI>http://purl.org/essglobal/standard/legal-form/</VocabularyEncodingSchemeURI>
	  		 </NonLiteralConstraint>
      </StatementTemplate>
      
      <StatementTemplate ID="has-Product-or-Service" minOccurs="0" maxOccurs="infinity" type="nonliteral">
            <Property>http://purl.org/essglobal/vocab/v1.0/hasProductOrService</Property>
            <NonLiteralConstraint descriptionTemplateRef="Product-or-Service"/>
      </StatementTemplate>
         
         <StatementTemplate ID="Is-made-of-SSE-Initiatives" minOccurs="1" maxOccurs="infinity" type="nonliteral">
            <Property>http://purl.org/dc/terms/hasPart</Property>
            <NonLiteralConstraint descriptionTemplateRef="SSEInitiative"/>
      </StatementTemplate>
      
      
  </DescriptionTemplate> 


<DescriptionTemplate ID="Product-or-Service" minOccur="0" maxOccur="infinity" standalone="yes">
    <ResourceClass>http://purl.org/essglobal/vocab/v1.0/ProductOrService</ResourceClass>
    
    <StatementTemplate ID="Name"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/goodrelations/v1#name</Property>
    </StatementTemplate>
 
    <StatementTemplate ID="Description"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://www.purl.org/dc/terms/description</Property>
	   <LiteralConstraint>
	  <LanguageOccurrence>optional</LanguageOccurrence>
	</LiteralConstraint>
    </StatementTemplate>
 
 	 <StatementTemplate ID="Category"  minOccurs="0" maxOccurs="infinity" type="literal">
		<Property>http://purl.org/goodrelations/v1#category</Property>	   
	    <NonLiteralConstraint> 
	    		<VocabularyEncodingSchemeURI>http://purl.org/essglobal/standard/products-services/</VocabularyEncodingSchemeURI>
	    </NonLiteralConstraint>
	    </StatementTemplate>

	<StatementTemplate ID="Qualifier"  minOccurs="0" maxOccurs="infinity" type="nonliteral">
	  <Property>http://www.purl.org/essglobal/vocab/v1.0/qualifier</Property>
	  <NonLiteralConstraint> 
	    <VocabularyEncodingSchemeURI>http://purl.org/essglobal/standard/qualifier/</VocabularyEncodingSchemeURI>
	  </NonLiteralConstraint>
      </StatementTemplate>    
    
   <StatementTemplate ID="Image"  minOccurs="0" maxOccurs="1" type="nonliteral">
	   <Property>http://xmlns.com/foaf/0.1/depicts</Property> 
    </StatementTemplate>
    
    <StatementTemplate ID="Unit"  minOccurs="0" maxOccurs="1" type="literal">
	   <Property>http://purl.org/goodrelations/v1#hasUnitOfMeasurement</Property> 
    </StatementTemplate>
    
    <StatementTemplate ID="has-sale-Option" minOccurs="0" maxOccurs="infinity" type="nonliteral">
            <Property>http://www.purl.org/essglobal/vocab/v1.0/hasSaleOption</Property>
            <NonLiteralConstraint descriptionTemplateRef="Sale-Option"/>
      </StatementTemplate>

    <StatementTemplate ID="has-Cost" minOccurs="0" maxOccurs="1" type="nonliteral">
            <Property>http://www.purl.org/essglobal/vocab/v1.0/hasCost</Property>
            <NonLiteralConstraint descriptionTemplateRef="Cost"/>
      </StatementTemplate>
           
</DescriptionTemplate> 


<DescriptionTemplate ID="Sale-Option" minOccur="0" maxOccur="infinity" standalone="no">
        
    <StatementTemplate ID="isavailable"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/isAvailable</Property>
	   <LiteralConstraint>
           <SyntaxEncodingScheme>http://schema.org/Boolean</SyntaxEncodingScheme>
     </LiteralConstraint>   
    </StatementTemplate>
    
        <StatementTemplate ID="Sales-Margin-Percentage"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/salesMarginPercentage</Property>
      </StatementTemplate>
    
    <StatementTemplate ID="Sales-Margin-Value"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/salesMarginValue</Property>
      </StatementTemplate>
      
      <StatementTemplate ID="Delivery-Cost"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/goodrelations/v1#hasCurrencyPrice</Property>
      </StatementTemplate>
      
  <StatementTemplate ID="Price"  minOccurs="1" maxOccurs="1" type="literal">
	   <Property>http://purl.org/goodrelations/v1#hasCurrencyPrice</Property> 
    </StatementTemplate>
    
<StatementTemplate ID="is-available-at-a-Location-of-Sale" minOccurs="1" maxOccurs="1" type="nonliteral">
            <Property>http://purl.org/essglobal/vocab/v1.0/availableAt</Property>
            <NonLiteralConstraint descriptionTemplateRef="Location-of-Sale"/>
      </StatementTemplate>
    
</DescriptionTemplate> 


<DescriptionTemplate ID="Location-of-Sale" minOccur="0" maxOccur="infinity" standalone="yes">
    <ResourceClass>http://purl.org/essglobal/vocab/v1.0/LocationOfSale</ResourceClass>

  <StatementTemplate ID="Name"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://www.purl.org/dc/terms/title</Property>
  </StatementTemplate>

  <StatementTemplate ID="Address"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://www.w3.org/2006/vcard/ns/street-address</Property>
  </StatementTemplate>

  <StatementTemplate ID="Zip-Code"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://www.w3.org/2006/vcard/ns/postal-code</Property>
  </StatementTemplate>
  
    <StatementTemplate ID="State"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/state</Property>
  </StatementTemplate>

  <StatementTemplate ID="City"  minOccurs="0" maxOccurs="1" type="nonliteral">
	  <Property>http://www.w3.org/2006/vcard/ns/locality</Property>
	  <NonLiteralConstraint>	
       <VocabularyEncodingSchemeURI>http://purl.org/dc/terms/TGN</VocabularyEncodingSchemeURI>
	</NonLiteralConstraint>
  </StatementTemplate>

  <StatementTemplate ID="Country"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://www.w3.org/2006/vcard/ns/country-name</Property>
	  <LiteralConstraint>
           <SyntaxEncodingScheme>http://purl.org/dc/terms/ISO3166</SyntaxEncodingScheme>
     </LiteralConstraint>
  </StatementTemplate>

   <StatementTemplate ID="isMain"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/isMain</Property>
	   <LiteralConstraint>
           <SyntaxEncodingScheme>http://schema.org/Boolean</SyntaxEncodingScheme>
     </LiteralConstraint>
  </StatementTemplate>
  

   <StatementTemplate ID="isSSE"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/isSSE</Property>
	   <LiteralConstraint>
           <SyntaxEncodingScheme>http://schema.org/Boolean</SyntaxEncodingScheme>
     </LiteralConstraint>
  </StatementTemplate>

  
  <StatementTemplate ID="Mobile-Phone"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://schema.org/telephone</Property>
  </StatementTemplate>

  <StatementTemplate ID="Phone"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://schema.org/telephone</Property>
  </StatementTemplate>
  
  <StatementTemplate ID="Email" minOccurs="0" maxOccurs="1" type="nonliteral">
        <Property>http://xmlns.com/foaf/0.1/mbox</Property>
        <NonLiteralConstraint>
          <ValueURIOccurrence>mandatory</ValueURIOccurrence>
        </NonLiteralConstraint>
      </StatementTemplate>

  <StatementTemplate ID="Fax"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://schema.org/faxnumber</Property>
  </StatementTemplate>
  
  <StatementTemplate ID="Delivery-Range-Distance"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/delivery-range-distance</Property>
  </StatementTemplate>
  
  <StatementTemplate ID="Delivery-Range-Geographic-Area"  minOccurs="0" maxOccurs="infinity" type="literal">
	  <Property>http://purl.org/goodrelations/v1#eligibleRegions</Property>
	  <LiteralConstraint>
           <SyntaxEncodingScheme>http://purl.org/dc/terms/ISO3166</SyntaxEncodingScheme>
     </LiteralConstraint>
  </StatementTemplate>
  
  <StatementTemplate ID="Longitude"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://www.w3.org/2006/vcard/ns/logitude</Property>
  </StatementTemplate>

  <StatementTemplate ID="Latitude"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://www.w3.org/2006/vcard/ns/latitude</Property>
  </StatementTemplate>
     
</DescriptionTemplate>

<DescriptionTemplate ID="Cost" minOccur="0" maxOccur="1" standalone="no">
    <ResourceClass>http://purl.org/essglobal/vocab/v1.0/Cost</ResourceClass>

    <StatementTemplate ID="Value"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/goodrelations/v1#hasCurrencyValue</Property>
    </StatementTemplate>
    
    <StatementTemplate ID="Currency"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/goodrelations/v1#hasCurrency</Property>
    </StatementTemplate>
    
    <StatementTemplate ID="is-composed-by" minOccurs="0" maxOccurs="infinity" type="nonliteral">
    <Property>http://purl.org/essglobal/vocab/v1.0/isComposedBy</Property>
               <NonLiteralConstraint descriptionTemplateRef="Cost-Composition"/>
      </StatementTemplate>

</DescriptionTemplate>


<DescriptionTemplate ID="Cost-Composition" minOccur="0" maxOccur="1" standalone="no">
        <ResourceClass>http://purl.org/essglobal/vocab/v1.0/CostComposition</ResourceClass>
        
    <StatementTemplate ID="Has-Input" minOccurs="0" maxOccurs="infinity" type="nonliteral">
    <Property>http://purl.org/essglobal/vocab/v1.0/hasInputCost</Property>
               <NonLiteralConstraint descriptionTemplateRef="Input"/>
      </StatementTemplate>
      
      <StatementTemplate ID="Has-Labour" minOccurs="0" maxOccurs="infinity" type="nonliteral">
    <Property>http://purl.org/essglobal/vocab/v1.0/hasLabourCost</Property>
               <NonLiteralConstraint descriptionTemplateRef="Labour"/>
      </StatementTemplate>
      
      <StatementTemplate ID="Has-Other-Costs" minOccurs="0" maxOccurs="infinity" type="nonliteral">
    <Property>http://purl.org/essglobal/vocab/v1.0/hasOtherCosts</Property>
               <NonLiteralConstraint descriptionTemplateRef="Other-Costs"/>
      </StatementTemplate>  
</DescriptionTemplate>


<DescriptionTemplate ID="Input" minOccur="0" maxOccur="infinity" standalone="no">
    <ResourceClass>http://purl.org/essglobal/vocab/v1.0/Input</ResourceClass>

    <StatementTemplate ID="Description"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/dc/terms/description</Property>
     <LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
    </StatementTemplate>

 	<StatementTemplate ID="Quantity-per-ProductOrService-Unit"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/quantityPerProductOrServiceUnit</Property>
    </StatementTemplate>

	<StatementTemplate ID="Cost-per-Unit"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/costPerUnit</Property>
    </StatementTemplate>

	<StatementTemplate ID="Unit"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/unit</Property>
    </StatementTemplate>
    
    <StatementTemplate ID="Category"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://purl.org/goodrelations/v1#category</Property>
		<NonLiteralConstraint> 
	    <VocabularyEncodingSchemeURI>http://purl.org/essglobal/standard/products-services/</VocabularyEncodingSchemeURI>
	  </NonLiteralConstraint>
    </StatementTemplate>

	<StatementTemplate ID="Qualifier"  minOccurs="0" maxOccurs="1" type="nonliteral">
	  <Property>http://www.purl.org/essglobal/vocab/v1.0/qualifier</Property>
	  <NonLiteralConstraint> 
	    <VocabularyEncodingSchemeURI>http://purl.org/essglobal/standard/qualifier/</VocabularyEncodingSchemeURI>
	  </NonLiteralConstraint>
      </StatementTemplate> 
      
    <StatementTemplate ID="Has-Impact-on-Price"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/hasImpactOnPrice</Property>
	   <LiteralConstraint>
           <SyntaxEncodingScheme>http://schema.org/Boolean</SyntaxEncodingScheme>
     </LiteralConstraint>
    </StatementTemplate>

</DescriptionTemplate>



<DescriptionTemplate ID="Labour" minOccur="0" maxOccur="infinity" standalone="no">
    <ResourceClass>http://purl.org/essglobal/vocab/v1.0/Labour</ResourceClass>
    
	<StatementTemplate ID="Description"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/dc/terms/description</Property>
     <LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
    </StatementTemplate>

    <StatementTemplate ID="Type"  minOccurs="0" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/typeOfLabour<Property>
    </StatementTemplate>

   
 <StatementTemplate ID="Contribution-to-the-Cost"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/goodrelations/v1#hasCurrencyValue</Property>
    </StatementTemplate>

</DescriptionTemplate>


<DescriptionTemplate ID="Other-Costs" minOccur="0" maxOccur="infinity" standalone="no">
    <ResourceClass>http://purl.org/essglobal/vocab/v1.0/OtherCosts</ResourceClass>

	<StatementTemplate ID="Description"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/dc/terms/description</Property>
     <LiteralConstraint>
	    <LanguageOccurrence>optional</LanguageOccurrence>
	  </LiteralConstraint>
    </StatementTemplate>

 <StatementTemplate ID="Contribution-to-the-Cost"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/goodrelations/v1#hasCurrencyValue</Property>
    </StatementTemplate>


  <StatementTemplate ID="Has-Impact-on-Price"  minOccurs="1" maxOccurs="1" type="literal">
	  <Property>http://purl.org/essglobal/vocab/v1.0/hasImpactOnPrice</Property>
	   <LiteralConstraint>
           <SyntaxEncodingScheme>http://schema.org/Boolean</SyntaxEncodingScheme>
     </LiteralConstraint>
    </StatementTemplate>

</DescriptionTemplate>


</DescriptionSetTemplate>